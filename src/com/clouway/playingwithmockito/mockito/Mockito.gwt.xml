<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Mockito">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.clouway.playingwithmockito.mockito.client.Mockito'/>

    <source path="shared"/>
    <source path="client"/>
    <!-- Specify the app servlets.                   -->
    <servlet path='/MockitoService' class='com.clouway.playingwithmockito.mockito.server.MockitoServiceImpl'/>
    <!--<servlet path="/CompanySearch" class="com.clouway.playingwithmockito.mockito.server.CompanySearchImpl"/>-->

</module>